    margin-bottom: 1em;
    margin-left: 0;
    margin-right: 0;
    padding-left: 40px;
}
</style>

<div id="ide">
<div class="product-grid" >
	
	
    @foreach (var y in Model)
    {
      
       <ul>
        <li><img src="~/Images/@y.image"/></li>
	    <li><b>Title:</b>@y.title </li> 
        <li><b>Price:</b>@y.price </li>
        <li><b>Phone#:</b>@y.phoneno</li>
